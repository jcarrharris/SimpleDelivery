<div class="section-content">
  <span>
    <h1><%= @business.business_name %></h1>
    <%= link_to "Edit", edit_business_path(@business), class:"diff"%>
    <%= link_to "New Location", new_business_location_path(@business), class:"button" %>
  </span>
  <div class="picture"><%= image_tag @business.image_url.to_s %></div>
  <div class="row">
    <div class="col-sm-6 col-md-3">
      <a href="#" class="thumbnail">
        <img data-src="holder.js/100%x180" alt="...">
      </a>
    </div>
  </div>
  <table class="table table-hover">
    <tbody>
      <tr><td>Website</td><td class="value"> <%= @business.website_url %></td></tr>
      <tr><td>Phone Number</td><td class="value"> <%= @business.phone_number %></td></tr>
    </tbody>
  </table>
</div>