<h1><%= @business.business_name %> </h1>

<div class="userinfo">
<%= link_to "Edit", edit_business_path(@business) %>
<%= link_to "Delete", @business, :confirm => "Are you sure you want to delete this business?", method: :delete %>
</br>
<%= link_to "Create new location", new_business_location_path(@business) %>
</div>
<div class="picture">
  <%= image_tag @business.image_url.to_s %>
<div class="userinformation">
	<div class="topic">Business:</div> <%= @business.business_name %>
	<div class="topic">Website:</div> <%= @business.website_url %>
	<div class="topic">Phone Number:</div> <%= @business.phone_number %>
</div>