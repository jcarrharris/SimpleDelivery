<!DOCTYPE html>
<html>

<%= render 'layouts/nav' %>
<head>
  <title>SimpleDelivery</title>
  <%= include_gon %>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <ul class="message_container">
  	<% if flash[:notice] %>
      <p id="notice"><%= flash[:notice] %></p>
    <% end %>
    <% if flash[:alert] %>
      <p id="alert"><%= flash[:alert] %></p>
    <% end %>
  </ul>

	<%= yield %>

  <div class="userinfo">
    <% if user_signed_in? %>
    <% else %>
      <%= link_to "Sign up", new_user_registration_path %> or <%= link_to "Log in", new_user_session_path %>
    <% end %>
  </div>

  <script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyA9JBz-80iyKF7JGzeikG0zHHqWdF2CLPs&sensor=true"></script>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
</body>
<div id="footer">Footer Goes Here </div>  
</html>