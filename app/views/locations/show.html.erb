<h1><%= @location.location_name %> </h1>

<div class="userinfo">
<%= link_to "Edit", edit_user_business_location_path(@user, @business, @location) %>
<%= link_to "Delete", [@user, @business, @location], :confirm => "Are you sure you want to delete this location?", method: :delete %> </br>
<%= link_to "View your orders", user_business_location_orders_path(@user, @business, @location) %>
</div>

<div class="userinformation">
  <p>Location: <%= @location.location_name %></p>
  <p>Address: <%= @location.address %></p>
  <p>Coordinates: <%= Geocoder.coordinates(@location.address) %></p>
  <p>Phone Number: <%= @location.phone_number %></p>
</div>

<div class="location_map">
 
</div>
