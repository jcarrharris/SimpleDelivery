<h1>Locations</h1>
  <% current_user.businesses.each do |b| %>
    <div class="header"> <%= link_to b.business_name, business_path(b) %> </div>
    <% b.locations.each do |l| %>
      <div class="userinformation">
        <p>Location: <%= link_to l.location_name, business_location_path(b, l) %></p>
        <p>Address: <%= l.address %></p>
        <p>Coordinates: <%= Geocoder.coordinates(l.address) %></p>
        <p>Phone Number: <%= l.phone_number %></p>
      </div>
    <% end %>
  <% end %>